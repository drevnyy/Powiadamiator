<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.very.wow.www.powiadamiator">

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitiesInDay"
            android:label="@string/title_activity_activities_in_day"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.very.wow.www.powiadamiator.MainActivity" />
        </activity>
        <activity
            android:name=".AllSubjectss"
            android:label="@string/title_activity_all_subjectss"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.very.wow.www.powiadamiator.MainActivity" />
        </activity>
        <activity android:name=".AddSubject" />
        <activity
            android:name=".SubjectDetails"
            android:label="@string/title_activity_subject_details"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.very.wow.www.powiadamiator.MainActivity" />
        </activity>
        <activity
            android:name=".AddTerminFromDays"
            android:label="@string/title_activity_add_termin_from_days"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.very.wow.www.powiadamiator.MainActivity" />
        </activity>
        <activity
            android:name=".AddTerminFromSubject"
            android:label="@string/title_activity_add_termin_from_subject"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.very.wow.www.powiadamiator.MainActivity" />
        </activity>
    </application>

</manifest>